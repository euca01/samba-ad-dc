#!/usr/bin/expect

spawn samba-tool gpo admxload -U Administrator --admx-dir=/opt/PolicyDefinitions/
expect "Enter KDC database master key: "
send -- "$env(KERBEROS_PASSWORD)\r"
expect "Re-enter KDC database master key to verify: "
send -- "$env(KERBEROS_PASSWORD)\r"
expect "\r"
send -- "exit\r"
expect eof
